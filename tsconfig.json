{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "types": ["node", "express"]
  },
  "include": ["src", "prisma"]
}
